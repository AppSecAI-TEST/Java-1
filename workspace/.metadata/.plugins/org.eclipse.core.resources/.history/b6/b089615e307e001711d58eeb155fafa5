package array.primitive.test;

public class IntArrayTest_1 {

	public static void main(String[] args) {
		int[] arr;
		arr = new int[3];
		
		printArray(arr);
		
		arr[0] = 11;
		arr[1] = 22;
		arr[2] = 33;
		
		printArray(arr);
	}

	private static void printArray(int[] arr) {
		for(int i = 0; i < 3; i++){
			System.out.print("[" + "i : " + arr[i] + "]\t");
		}
	}
	
	

}
