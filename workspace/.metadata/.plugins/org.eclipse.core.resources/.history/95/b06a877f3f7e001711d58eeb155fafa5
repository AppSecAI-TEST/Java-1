package array.primitive.test1;

public class FindMaxMinTest {
	public static void main(String[] args){
		int[] score = {79, 88, 91, 33, 100, 55, 95};
		int max = 0;
		int min = 1000;
		
		for(int i = 0; i < score.length; i++)
			if (score[i] > max)
				max = score[i];
		
		System.out.println(max);
	}
}
