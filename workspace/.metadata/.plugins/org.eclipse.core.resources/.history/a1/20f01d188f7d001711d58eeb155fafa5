package capsulation.test;

class MyDate {
	private int month;
	private int day;
	
	public int getMonth() {
		return month;
	}
	public void setMonth(int month) {
		if(month < 1 && month > 12)
		this.month = month;
	}
	public int getDay() {
		return day;
	}
	public void setDay(int day) {
		this.day = day;
	}
	
	
}
public class MyDateTest {

	public static void main(String[] args) {
		

	}

}
